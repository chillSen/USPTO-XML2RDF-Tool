&lt;http://us.patents.aksw.org/patent/<xsl:value-of select="document-id/doc-number"/>&gt;    &lt;http://us.patents.aksw.org/schema/hasName&gt;  "<xsl:value-of select="document-id/name"/>"@en  .
<xsl:if test="substring(document-id/date,7,9) != '00'">&lt;http://us.patents.aksw.org/patent/<xsl:value-of select="document-id/doc-number"/>&gt;    &lt;http://purl/org/dc/terms/date&gt;  "<xsl:value-of select="substring(document-id/date,0,5)"/>-<xsl:value-of select="substring(document-id/date,5,2)"/>-<xsl:value-of select="substring(document-id/date,7,9)"/>"^^xsd:date  .
</xsl:if><xsl:if test="substring(document-id/date,7,9) = '00'">&lt;http://us.patents.aksw.org/patent/<xsl:value-of select="document-id/doc-number"/>&gt;    &lt;http://purl/org/dc/terms/date&gt;  "<xsl:value-of select="substring(document-id/date,0,5)"/>-<xsl:value-of select="substring(document-id/date,5,2)"/>-01"^^xsd:date  .
