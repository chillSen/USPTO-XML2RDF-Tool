&lt;http://us.patents.aksw.org/patent/<xsl:value-of select="publication-reference/document-id/doc-number"/>/<xsl:value-of select="publication-reference/document-id/country"/>&gt;    &lt;http://www.w3.org/2000/01/rdf-schema#label&gt;  "<xsl:value-of select="publication-reference/document-id/country"/>"@en  .
&lt;http://us.patents.aksw.org/patent/<xsl:value-of select="publication-reference/document-id/doc-number"/>&gt;    &lt;http://us.patents.aksw.org/schema/kind&gt;  "<xsl:value-of select="publication-reference/document-id/kind"/>"@en  .
