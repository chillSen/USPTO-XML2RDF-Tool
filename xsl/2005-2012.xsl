&lt;http://us.patents.aksw.org/applicant/<xsl:value-of select="../../../publication-reference/document-id/doc-number"/>-<xsl:value-of select="addressbook/last-name"/>&gt;   &lt;http://us.patents.aksw.org/schema/state&gt;  "<xsl:value-of select="addressbook/address/state"/>"@en  .
&lt;http://us.patents.aksw.org/applicant/<xsl:value-of select="../../../publication-reference/document-id/doc-number"/>-<xsl:value-of select="addressbook/last-name"/>&gt;   &lt;http://us.patents.aksw.org/schema/hasCountry&gt;  &lt;http://us.patents.aksw.org/applicant/<xsl:value-of select="../../../publication-reference/document-id/doc-number"/>-<xsl:value-of select="addressbook/last-name"/>/<xsl:value-of select="addressbook/address/country"/>&gt;  .
&lt;http://us.patents.aksw.org/applicant/<xsl:value-of select="../../../publication-reference/document-id/doc-number"/>-<xsl:value-of select="addressbook/last-name"/>/<xsl:value-of select="addressbook/address/country"/>&gt;    &lt;http://www.w3.org/2000/01/rdf-schema#label&gt;  "<xsl:value-of select="addressbook/address/country"/>"@en  .
</xsl:for-each> 
</xsl:for-each> 
</xsl:template>
</xsl:stylesheet>
